{
  "applicationId": "dsm-tool",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/my-vue-app/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "Dsm tool",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/my-vue-app/public/logo.ico",
      "enableInspector":false,
      "port":3001
    }
  },
  "cli": {
    "binaryName": "dsm-tool",
    "resourcesPath": "/my-vue-app/dist",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/my-vue-app/public/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0",
    "frontendLibrary":{
      "patchFile":"/my-vue-app/index.html",
      "devUrl":"http://localhost:3000"
    }
  }
}